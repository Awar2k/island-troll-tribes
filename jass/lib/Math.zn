library Math {
	public function RealMax(real a, real b) -> real {
		if (a < b) {
			return b;
		}
		return a;
	}

	public function RealMin(real a, real b) -> real {
		if (a > b) {
			return b;
		}
		return a;
	}

	public function EuclideanDistance(real x, real y) -> real {
		return SquareRoot(Pow(x, 2) + Pow(y, 2));
	}
}