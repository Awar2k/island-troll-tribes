library v3000e requires ChangeLog {
  function onInit() {
    ChangeLog changes = ChangeLog.create(3, 0, 'e');
    changes.add("Boats now stop at 4 points around the middle circle for 3 seconds, at a point in front of each major island");
    changes.add("Boat paths are now fixed, they go around the center island and back to where they spawned");
    changes.add("The first boats will either be from N and E or S and W and then they alternate");
    changes.add("The north-west island now has a new ramp connecting to north-east");
    changes.add("The class-selection totems have been moved towards the center of the island");
  }
}